$data = loadMenuData();
$menus = $data['menus'];

function renderMenu($menus, $isSidebar = false, $isDropdown = false, $parentID = '') {
    $html = $isSidebar 
        ? ($isDropdown ? '<div class="collapse" id="subsubmenu_' . $parentID . '"><ul class="flex-column pl-2 nav">' : '<ul class="nav flex-column flex-nowrap" id="ulmenu">')
        : ($isDropdown ? '<ul class="sub-menu">' : '<ul>');

    foreach ($menus as $index => $menu) {
        $hasChildren = isset($menu['children']) && !empty($menu['children']);
        $safeName = strtolower(preg_replace('/[^a-zA-Z0-9-_]/', '-', $menu['name']));
        $uniqueId = $parentID ? $parentID . '-' . $safeName : $safeName;

        if ($isSidebar) {
            $dropdownClass = $hasChildren ? 'nav-item mainDropdown' : 'nav-item';
            $caret = $hasChildren ? ' <span class="caret"></span>' : ''; // Define $caret

            $html .= '<li class="' . $dropdownClass . '">';
            $html .= '<a href="' . $menu['url'] . '"' . ($hasChildren ? ' class="nav-link collapsed" aria-expanded="false"' : '') . '>';
            $html .= htmlspecialchars($menu['name']) . $caret . '</a>';
            $html .= $hasChildren ? '<span role="button" data-toggle="collapse" data-target="#subsubmenu_' . $uniqueId . '" class="fa fa-plus sub" aria-expanded="false" aria-label="Plus Icon"></span>' : '';
        } else {
            $dropdownClass = $hasChildren ? 'dropdown' : '';
            $caret = $hasChildren ? ' <span class="caret"></span>' : ''; // Define $caret

            $html .= '<li class="' . $dropdownClass . '">';
            $html .= '<a href="' . $menu['url'] . '"' . ($hasChildren ? ' ' : '') . '>';
            $html .= htmlspecialchars($menu['name']) . $caret . '</a>';
        }

        if ($hasChildren) {
            $html .= renderMenu($menu['children'], $isSidebar, true, $uniqueId);
        }

        $html .= '</li>';
    }

    $html .= $isSidebar && $isDropdown ? '</ul></div>' : '</ul>';
    return $html;
}

